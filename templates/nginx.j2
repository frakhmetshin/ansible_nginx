user root;
worker_processes auto;
pid /run/nginx.pid;
include /etc/nginx/modules-enabled/*.conf;

events {
        worker_connections 768;
        # multi_accept on;
}

http {
server {
        listen 8081;
        server_name fartest;
        access_log /var/log/nginx/fartest/access.log;
        error_log /var/log/nginx/fartest/error.log;
        include /etc/nginx/configs/*.conf
        gzip on;
        
}

}


